{
  "displayName": "Homebridge WattBox Plugin",
  "name": "homebridge-wattbox",
  "version": "2.0.3",
  "description": "WattBox plugin for Homebridge.",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/derek-miller/homebridge-wattbox.git"
  },
  "bugs": {
    "url": "https://github.com/derek-miller/homebridge-wattbox/issues"
  },
  "engines": {
    "node": ">=14.18.1",
    "homebridge": ">=1.3.5"
  },
  "main": "dist/index.js",
  "scripts": {
    "eslint": "eslint src/**.ts --max-warnings=0",
    "eslint-fix": "eslint src/**.ts --max-warnings=0 --fix",
    "prettier": "prettier --check src *.{yaml,json,md}",
    "prettier-fix": "prettier --write src *.{yaml,json,md}",
    "lint": "npm run eslint && npm run prettier",
    "lint-fix": "npm run eslint-fix && npm run prettier-fix",
    "fmt": "npm run lint-fix",
    "watch": "npm run build && npm link && nodemon",
    "build": "rm -rf ./dist && tsc",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "keywords": [
    "homebridge-plugin",
    "wattbox"
  ],
  "dependencies": {
    "async-lock": "^1.4.0",
    "axios": "^1.4.0",
    "axios-retry": "^3.5.1",
    "cache-manager": "^5.2.3",
    "pubsub-js": "^1.9.4",
    "xml2js": "^0.6.0"
  },
  "devDependencies": {
    "@types/async-lock": "^1.4.0",
    "@types/node": "^20.3.2",
    "@types/pubsub-js": "^1.8.3",
    "@types/xml2js": "^0.4.11",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.43.0",
    "homebridge": "^1.6.1",
    "homebridge-config-ui-x": "^4.50.4",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.5"
  }
}
